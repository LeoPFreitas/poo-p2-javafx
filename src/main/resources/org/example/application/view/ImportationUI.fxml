<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="323.0" prefWidth="730.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.application.controller.ImportationUIController">

    <Label fx:id="lbPersonId" layoutX="8.0" layoutY="96.0" text="ID Pessoa:"/>
    <Label fx:id="lbName" layoutX="32.0" layoutY="141.0" text="Nome:"/>
    <Label fx:id="lbPrice" layoutX="34.0" layoutY="186.0" text="Preço:"/>
    <Label fx:id="lbValidatedPerson" layoutX="313.0" layoutY="97.0"/>
    <Label fx:id="lbCategory" layoutX="389.0" layoutY="141.0" prefHeight="16.0" prefWidth="68.0" text="Categoria:"/>
    <TextField fx:id="txtPersonID" layoutX="86.0" layoutY="92.0" prefHeight="26.0" prefWidth="119.0"/>
    <TextField fx:id="txtName" layoutX="86.0" layoutY="136.0" prefHeight="26.0" prefWidth="246.0"/>
    <TextField fx:id="txtPrice" layoutX="86.0" layoutY="181.0" prefHeight="26.0" prefWidth="246.0"/>
    <TextField fx:id="txtWeight" layoutX="467.0" layoutY="181.0" prefHeight="26.0" prefWidth="246.0"/>
    <Button fx:id="btnImport" layoutX="625.0" layoutY="261.0" mnemonicParsing="false" onAction="#importProduct"
            prefHeight="39.0" prefWidth="88.0" text="Salvar"/>
    <Button fx:id="btnValidatePerson" layoutX="214.0" layoutY="92.0" mnemonicParsing="false" onAction="#validatePerson"
            prefHeight="26.0" prefWidth="88.0" text="Validar"/>
    <Button fx:id="btnCancel" layoutX="518.0" layoutY="261.0" mnemonicParsing="false" onAction="#backToPreviousScene"
            prefHeight="39.0" prefWidth="88.0" text="Cancelar"/>
    <ComboBox fx:id="cbxCategory" layoutX="467.0" layoutY="136.0" prefWidth="150.0"/>
    <Label fx:id="lbWeight" layoutX="421.0" layoutY="186.0" text="Peso:"/>
    <Label layoutX="261.0" layoutY="21.0" text="Painel de Importação">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>

</AnchorPane>
